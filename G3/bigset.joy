bs_insert == unitlist bs_union;
bs_new == [];
bs_union ==
        [ [ [null] pop ]
          [ [pop null] swap pop ]
          [ [unswons2 [first] dip first <] [uncons] dip bs_union cons ]
          [ [unswons2 [first] dip first >] uncons swapd bs_union cons ]
          [ rest [uncons] dip bs_union cons ] ]
        cond;
